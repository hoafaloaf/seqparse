[run]
omit =
    *argparse*
    *builtins.py
    *ctypes*
    *decimal.py
    *encodings*
    *fractions.py
    *funcsigs*
    *future*
    *humanize*
    *mock*
    *nose*
    *numbers.py
    *packaging*
    *pkg_resources*
    *pbr*
    *pyparsing*
    *scandir*
    *six.py
    *seqparse.test*
    *uuid.py

[report]
exclude_lines =
    pragma: no cover
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
